(instantiate-prototype 'texture-toggle-button "./res/main-menu/play.png"
                       :hover "./res/main-menu/play-hover.png"
                       :pressed "./res/main-menu/play-press.png"
                       :anchor (v2! -0.33 0)
                       :pos (v2! 0 0)
                       :on-pressed
                       (lambda (id pos)
                         (declare (ignore id pos))
                         (load-scene "./res/lvl/square.lvl")))

